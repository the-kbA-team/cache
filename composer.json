  "type": "library",
  "support": {
    "issues": "https://github.com/the-kbA-team/cache/issues"
  },
  "require": {
    "php": ">=5.5",
    "predis/predis": "~1.1.1",
    "psr/simple-cache": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "kbATeam\\Cache\\": "src/"
    }
  },
  "autoload-dev": {

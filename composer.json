{
  "name": "kba-team/cache",
  "description": "Simple cache implementing PSR-16 connecting to Redis using PhpRedis.",
  "license": "MIT",
  "type": "library",
  "support": {
    "issues": "https://github.com/the-kbA-team/cache/issues"
  },
  "keywords": [
    "simple",
    "cache",
    "psr-16",
    "redis",
    "predis"
  ],
  "require": {
    "php": "^7.0",
    "ext-redis": "*",
    "psr/simple-cache": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "cache/integration-tests": "^0.16"
  },
  "provide": {
    "psr/simple-cache-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "kbATeam\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\kbATeam\\Cache\\": "tests/"
    }
  }
}
